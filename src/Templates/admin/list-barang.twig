{% extends 'layouts/dashboard_layouts.twig' %}

{% block dashboard_content %}
    <table class="" cellspacing="0" width="100%">
        <thead>
        <tr>
            <th>No.</th>
            <th>Nama Barang</th>
            <th>deskripsi</th>
        </tr>
        </thead>
        <tbody>
        {% if data|length>0 %}
            {% set i = 1 %}
            {% for dat in data %}
                <tr>
                    <td>{{ i }}</td>
                    <td>{{ dat.namaBarang }}</td>
                    <td>{{ dat.description }}</td>
                </tr>
            {% endfor %}
            {% set i = i + 1 %}
        {% else %}
        {% endif %}
        </tbody>
    </table>
{% endblock %}